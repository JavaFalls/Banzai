size_flags_vertical = 1
popup_exclusive = false
window_title = "Please Confirm..."
resizable = false
dialog_text = "Save and use these bots?"
dialog_hide_on_ok = true
_sections_unfolded = [ "Anchor", "Dialog", "Margin", "Popup", "Size Flags" ]

[connection signal="pressed" from="back_button" to="." method="_on_back_button_pressed"]

[connection signal="pressed" from="go_button" to="." method="_on_go_button_pressed"]

[connection signal="pressed" from="player_bot/Button" to="." method="switch_bot" binds= [ 0 ]]

[connection signal="pressed" from="training_bot/Button" to="." method="switch_bot" binds= [ 1 ]]

[connection signal="pressed" from="ai_builds/left" to="." method="switch_ai_bot" binds= [ -1 ]]

[connection signal="pressed" from="ai_builds/right" to="." method="switch_ai_bot" binds= [ 1 ]]

[connection signal="pressed" from="primary_weapons/HBoxContainer/left" to="." method="move_primaries" binds= [ "prev" ]]

[connection signal="pressed" from="primary_weapons/HBoxContainer/right" to="." method="move_primaries" binds= [ "next" ]]

[connection signal="pressed" from="secondary_weapons/HBoxContainer/left" to="." method="move_secondaries" binds= [ "prev" ]]

[connection signal="pressed" from="secondary_weapons/HBoxContainer/right" to="." method="move_secondaries" binds= [ "next" ]]

[connection signal="pressed" from="abilities/HBoxContainer/left" to="." method="move_abilities" binds= [ "prev" ]]

[connection signal="pressed" from="abilities/HBoxContainer/right" to="." method="move_abilities" binds= [ "next" ]]


